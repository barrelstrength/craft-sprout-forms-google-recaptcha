{
    "name": "barrelstrength/sprout-google-recaptcha",
    "description": "Google Recaptcha solution for Sprout Forms",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "sprout google recaptcha"
    ],
    "support": {
        "docs": "https://github.com/barrelstrength/craft-sprout-google-recaptcha /sprout-google-recaptcha/blob/master/README.md",
        "issues": "https://github.com/barrelstrength/craft-sprout-google-recaptcha /sprout-google-recaptcha/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Barrel Strength",
            "homepage": "https://www.barrelstrengthdesign.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "barrelstrength\\sproutgooglerecaptcha\\": "src/"
        }
    },
    "extra": {
        "name": "Sprout Google Recaptcha",
        "handle": "sprout-google-recaptcha",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/barrelstrength/craft-sprout-google-recaptcha /sprout-google-recaptcha/master/CHANGELOG.md",
        "components": {
            "app": "barrelstrength\\sproutgooglerecaptcha\\services\\App"
        },
        "class": "barrelstrength\\sproutgooglerecaptcha\\SproutGoogleRecaptcha"
    }
}
